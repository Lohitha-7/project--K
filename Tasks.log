<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>K-Guard AI | Universal Forensic Suite</title>
    <script src="https://cdn.tailwindcss.com"></script>
    <link href="https://fonts.googleapis.com/css2?family=Orbitron:wght@400;700&family=Rajdhani:wght@300;500;700&display=swap" rel="stylesheet">
    <style>
        body {
            background-color: #050b18;
            font-family: 'Rajdhani', sans-serif;
            color: #e2e8f0;
        }
        .header-font { font-family: 'Orbitron', sans-serif; }
        .neon-border {
            box-shadow: 0 0 15px rgba(0, 242, 255, 0.2);
            border: 1px solid rgba(0, 242, 255, 0.4);
        }
        .glow-text { text-shadow: 0 0 10px rgba(0, 242, 255, 0.8); }
        .scanning-line {
            height: 3px;
            background: linear-gradient(90deg, transparent, #00f2ff, transparent);
            position: absolute;
            width: 100%;
            top: 0;
            animation: scan 2.5s linear infinite;
            z-index: 20;
        }
        @keyframes scan {
            0% { top: 0%; }
            100% { top: 100%; }
        }
        .matrix-bg {
            background-image: radial-gradient(circle at 50% 50%, #0d1b2a 0%, #050b18 100%);
        }
        .pulse-border {
            animation: pulse-glow 2s infinite;
        }
        @keyframes pulse-glow {
            0%, 100% { border-color: rgba(0, 242, 255, 0.4); box-shadow: 0 0 5px rgba(0, 242, 255, 0.2); }
            50% { border-color: rgba(0, 242, 255, 0.9); box-shadow: 0 0 20px rgba(0, 242, 255, 0.5); }
        }
    </style>
</head>
<body class="matrix-bg min-h-screen p-4 md:p-8">

    <div class="max-w-7xl mx-auto">
        <!-- Header Section -->
        <header class="flex flex-col md:flex-row justify-between items-center mb-8 gap-4">
            <div class="text-left">
                <h1 class="header-font text-4xl font-bold text-cyan-400 glow-text mb-1">PROJECT K-GUARD</h1>
                <p class="text-cyan-100 tracking-[0.2em] text-xs uppercase opacity-70">Cross-Sector Forensic Integrity Engine</p>
            </div>
            <div class="flex gap-3">
                <div class="px-4 py-2 bg-slate-900/80 border border-cyan-500/30 rounded-lg text-center">
                    <p class="text-[10px] text-cyan-500 uppercase">Neural Load</p>
                    <p class="header-font text-sm text-white">94.2 TFlops</p>
                </div>
                <div class="px-4 py-2 bg-slate-900/80 border border-purple-500/30 rounded-lg text-center">
                    <p class="text-[10px] text-purple-500 uppercase">Q-Encryption</p>
                    <p class="header-font text-sm text-white">AES-Q512</p>
                </div>
            </div>
        </header>

        <div class="grid grid-cols-1 lg:grid-cols-12 gap-6">
            <!-- Left Sidebar: Controls -->
            <div class="lg:col-span-3 space-y-6">
                <div class="neon-border bg-slate-900/60 p-5 rounded-xl backdrop-blur-md">
                    <h3 class="header-font text-sm text-cyan-400 mb-4 flex items-center gap-2">
                        <span class="w-2 h-2 bg-cyan-400 rounded-full animate-ping"></span>
                        COMMAND TERMINAL
                    </h3>
                    
                    <div class="space-y-4">
                        <div>
                            <label class="text-[10px] text-slate-400 uppercase tracking-widest">Sector Selection</label>
                            <select id="analysisMode" class="w-full mt-1 bg-slate-800 border border-slate-700 p-2.5 rounded text-sm text-cyan-100 focus:border-cyan-500 outline-none">
                                <option value="legal">Legal (Property/Bond Papers)</option>
                                <option value="academic">Academic (Degrees/Transcripts)</option>
                                <option value="corporate">Corporate (Trade Agreements)</option>
                                <option value="banking">Banking (Drafts/Checks)</option>
                            </select>
                        </div>

                        <div id="dropZone" class="border-2 border-dashed border-slate-700 rounded-lg p-6 text-center hover:border-cyan-500 transition-all cursor-pointer bg-slate-800/30" onclick="document.getElementById('fileUpload').click()">
                            <input type="file" id="fileUpload" class="hidden" onchange="handleFileSelect(event)">
                            <div id="uploadUI">
                                <svg class="w-10 h-10 mx-auto text-slate-500 mb-2" fill="none" stroke="currentColor" viewBox="0 0 24 24"><path d="M9 12h6m-6 4h6m2 5H7a2 2 0 01-2-2V5a2 2 0 012-2h5.586a1 1 0 01.707.293l5.414 5.414a1 1 0 01.293.707V19a2 2 0 01-2 2z"></path></svg>
                                <p class="text-xs text-slate-400">Upload Original or Scan</p>
                            </div>
                            <p class="text-xs text-cyan-400 hidden" id="fileNameDisp"></p>
                        </div>

                        <button onclick="startAnalysis()" class="w-full bg-cyan-600 hover:bg-cyan-400 text-black font-bold py-3 rounded text-sm header-font transition-all shadow-[0_0_15px_rgba(0,242,255,0.4)]">
                            EXECUTE SCAN
                        </button>
                    </div>
                </div>

                <div class="neon-border bg-slate-900/60 p-5 rounded-xl">
                    <h3 class="header-font text-[10px] text-purple-400 mb-3 tracking-widest">REAL-TIME ENTROPY</h3>
                    <div class="flex items-end gap-1 h-12 mb-2" id="entropyVisualizer">
                        <!-- Bars generated by JS -->
                    </div>
                    <p class="text-[9px] text-slate-500 font-mono">HASHING NODE: 0xFD...2291</p>
                </div>
            </div>

            <!-- Center: Visual Analysis -->
            <div class="lg:col-span-6 space-y-6">
                <div class="neon-border bg-black rounded-xl h-[480px] relative overflow-hidden flex items-center justify-center group">
                    <div id="scanLine" class="scanning-line hidden"></div>
                    
                    <div id="emptyMsg" class="text-center z-10">
                        <div class="w-16 h-16 border-4 border-slate-800 border-t-cyan-500 rounded-full animate-spin mx-auto mb-4 opacity-20"></div>
                        <p class="header-font text-xl text-slate-600">IDLE: FEED DATA</p>
                    </div>

                    <img id="targetImg" class="hidden max-h-full max-w-full object-contain z-0 transition-all duration-700">
                    
                    <!-- AI Detection Overlays -->
                    <div id="detectOverlay" class="absolute inset-0 pointer-events-none z-30"></div>
                </div>
            </div>

            <!-- Right Sidebar: Diagnostics -->
            <div class="lg:col-span-3 space-y-6">
                <div class="neon-border bg-slate-900/60 p-5 rounded-xl h-full flex flex-col">
                    <h3 class="header-font text-sm text-cyan-400 mb-4">DIAGNOSTIC LOG</h3>
                    <div id="logDisplay" class="flex-grow font-mono text-[11px] space-y-2 overflow-y-auto max-h-[400px] text-slate-400 pr-2">
                        <p class="text-cyan-700">> Initialization complete.</p>
                        <p class="text-cyan-700">> Core: AI-Forensics-v4.2</p>
                        <p class="text-cyan-700">> Ready for Cross-Sector Input.</p>
                    </div>
                    
                    <div class="mt-4 pt-4 border-t border-slate-800">
                        <div class="flex justify-between text-[10px] mb-1">
                            <span>Processing Power</span>
                            <span class="text-cyan-400">Optimal</span>
                        </div>
                        <div class="w-full bg-slate-800 h-1 rounded-full overflow-hidden">
                            <div class="bg-cyan-500 h-full w-4/5 animate-pulse"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- Results Modal -->
    <div id="reportModal" class="hidden fixed inset-0 bg-black/95 backdrop-blur-sm flex items-center justify-center p-4 z-50">
        <div id="modalContainer" class="neon-border bg-slate-900 p-8 rounded-2xl max-w-2xl w-full border-t-4 shadow-2xl">
            <div class="flex justify-between items-start mb-6">
                <div>
                    <h2 id="modalStatus" class="header-font text-4xl font-bold italic"></h2>
                    <p id="modalSub" class="text-xs uppercase tracking-[0.3em] opacity-60"></p>
                </div>
                <div id="scoreRing" class="w-20 h-20 rounded-full border-4 flex items-center justify-center font-bold text-xl"></div>
            </div>

            <div class="grid grid-cols-1 md:grid-cols-2 gap-6 mb-8">
                <div class="space-y-4">
                    <h4 class="text-xs font-bold text-cyan-500 uppercase">AI Observations</h4>
                    <ul id="observationList" class="text-sm space-y-2 text-slate-300"></ul>
                </div>
                <div class="space-y-4">
                    <h4 class="text-xs font-bold text-cyan-500 uppercase">Verification Breakdown</h4>
                    <div id="breakdown" class="space-y-3"></div>
                </div>
            </div>

            <button onclick="closeModal()" class="w-full py-3 bg-slate-800 hover:bg-slate-700 border border-slate-600 rounded-lg text-white font-bold transition-all">DISMISS REPORT</button>
        </div>
    </div>

    <script>
        let isLoaded = false;
        
        // Setup Entropy Bars
        const entropyVis = document.getElementById('entropyVisualizer');
        for(let i=0; i<20; i++) {
            const bar = document.createElement('div');
            bar.className = 'flex-1 bg-purple-500/30 rounded-t transition-all duration-500';
            bar.style.height = Math.random() * 100 + '%';
            entropyVis.appendChild(bar);
        }

        setInterval(() => {
            Array.from(entropyVis.children).forEach(bar => {
                bar.style.height = Math.random() * 100 + '%';
                bar.style.opacity = Math.random() * 0.5 + 0.3;
            });
        }, 800);

        function handleFileSelect(e) {
            const file = e.target.files[0];
            if (file) {
                isLoaded = true;
                document.getElementById('uploadUI').classList.add('hidden');
                document.getElementById('fileNameDisp').innerText = `ID: ${file.name.substring(0,20)}...`;
                document.getElementById('fileNameDisp').classList.remove('hidden');
                document.getElementById('emptyMsg').classList.add('hidden');
                
                const reader = new FileReader();
                reader.onload = (ev) => {
                    const img = document.getElementById('targetImg');
                    img.src = ev.target.result;
                    img.classList.remove('hidden');
                    addLog(`Source loaded: ${file.name}`, 'text-blue-400');
                };
                reader.readAsDataURL(file);
            }
        }

        function addLog(msg, color='text-cyan-500') {
            const log = document.getElementById('logDisplay');
            const p = document.createElement('p');
            p.className = color;
            p.innerHTML = `> ${msg}`;
            log.prepend(p);
        }

        async function startAnalysis() {
            if (!isLoaded) return alert("Please upload a document for forensic analysis.");
            
            const scanLine = document.getElementById('scanLine');
            scanLine.classList.remove('hidden');
            
            addLog("Initializing Neural Engine...", "text-yellow-500");
            await sleep(800);
            addLog("Scanning Fiber Texture & Paper Density...", "text-yellow-500");
            drawDetections();
            await sleep(1200);
            addLog("Comparing Micro-printing against original database...", "text-yellow-500");
            await sleep(1000);
            addLog("Quantum Hash Verification triggered...", "text-purple-400");
            await sleep(800);
            
            scanLine.classList.add('hidden');
            finalizeReport();
        }

        function drawDetections() {
            const overlay = document.getElementById('detectOverlay');
            overlay.innerHTML = `
                <div class="absolute border-2 border-red-500/50 bg-red-500/10 top-[20%] left-[15%] w-[30%] h-[15%] animate-pulse">
                    <span class="absolute -top-5 left-0 text-[10px] text-red-500 font-bold">STAMP_ANOMALY_01</span>
                </div>
                <div class="absolute border-2 border-cyan-500/50 bg-cyan-500/10 bottom-[20%] right-[15%] w-[40%] h-[10%]">
                    <span class="absolute -top-5 left-0 text-[10px] text-cyan-500 font-bold">SIG_REPLICATION_DETECTED</span>
                </div>
            `;
        }

        function finalizeReport() {
            const mode = document.getElementById('analysisMode').value;
            const modal = document.getElementById('reportModal');
            const status = document.getElementById('modalStatus');
            const sub = document.getElementById('modalSub');
            const ring = document.getElementById('scoreRing');
            const obs = document.getElementById('observationList');
            const breakdown = document.getElementById('breakdown');
            const container = document.getElementById('modalContainer');

            modal.classList.remove('hidden');

            // Logic to simulate "Original" vs "Fraud"
            // Tip for User: You can toggle this logic based on file name or mode
            const isOriginal = Math.random() > 0.5; 

            if (isOriginal) {
                container.style.borderColor = "#22c55e";
                status.innerText = "ORIGINAL VERIFIED";
                status.className = "header-font text-4xl font-bold italic text-green-500 glow-text";
                sub.innerText = "Document Integrity: 100% Secure";
                ring.innerText = "98%";
                ring.className = "w-20 h-20 rounded-full border-4 border-green-500 flex items-center justify-center font-bold text-xl text-green-500";
                
                obs.innerHTML = `
                    <li>• Micro-printing texture matches official standards.</li>
                    <li>• Paper fiber randomness verified (Non-Digital).</li>
                    <li>• Ink-to-Paper absorption consistent with manual pen usage.</li>
                `;
                breakdown.innerHTML = createProgress("Texture Analysis", 99) + createProgress("Sign. Authenticity", 97) + createProgress("Quantum Hash Match", 100);
            } else {
                container.style.borderColor = "#ef4444";
                status.innerText = "FORGERY DETECTED";
                status.className = "header-font text-4xl font-bold italic text-red-500 glow-text";
                sub.innerText = "High Risk: Tampering Detected";
                ring.innerText = "24%";
                ring.className = "w-20 h-20 rounded-full border-4 border-red-500 flex items-center justify-center font-bold text-xl text-red-500";
                
                obs.innerHTML = `
                    <li class="text-red-400">• Pixel-level anomalies found in Government Seal.</li>
                    <li class="text-red-400">• Digital replication noise found in signature curves.</li>
                    <li class="text-red-400">• Metadata timestamp mismatch with physical scan.</li>
                `;
                breakdown.innerHTML = createProgress("Texture Analysis", 30, 'bg-red-500') + createProgress("Sign. Authenticity", 15, 'bg-red-500') + createProgress("Quantum Hash Match", 0, 'bg-red-500');
            }
        }

        function createProgress(label, val, color='bg-green-500') {
            return `
                <div class="text-[10px] uppercase flex justify-between mb-1"><span>${label}</span><span>${val}%</span></div>
                <div class="w-full bg-slate-800 h-1.5 rounded-full"><div class="${color} h-full" style="width: ${val}%"></div></div>
            `;
        }

        function closeModal() {
            document.getElementById('reportModal').classList.add('hidden');
            document.getElementById('detectOverlay').innerHTML = '';
            document.getElementById('targetImg').style.opacity = '1';
        }

        function sleep(ms) { return new Promise(res => setTimeout(res, ms)); }
    </script>
</body>
</html>
